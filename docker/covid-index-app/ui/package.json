    "svelte": "^3.16.0",
    "tailwindcss": "^1.1.4"
  },
  "description": "COVID-19 Index App",
  "scripts": {
    "rollup-dev": "rollup -c --environment DEV:true",
    "rollup-prod": "rollup -c --environment DEV:false",
    "build": "run-s build-remote-prod",
    "build-dev": "run-s lint rollup-dev",
    "build-prod": "run-s lint rollup-prod",
    "autobuild": "rollup -c -w --environment DEV:true",
    "dev": "run-p start:dev lint autobuild",
    "start:dev": "sirv public --dev",
    "lint": "eslint src",
    "jest": "jest",
    "test": "run-s lint jest",
    "atest": "jest -i",
    "build:tailwind": "tailwind build tailwind.css -o public/tailwind-gen.css"
  }
}
